var
Current_Proj_Lib:string

procedure Get_Project_Lib()
var
str_arr:array[4] of string
begin
    scan_str(model_file(),"\\/",str_arr[0],str_arr[1],str_arr[2],str_arr[3])
    Current_Proj_Lib = str_arr[0] + '/' + str_arr[1] + '/' + str_arr[2]
    sim_warning('Get_Project_Lib  : ' + Current_Proj_Lib)
end